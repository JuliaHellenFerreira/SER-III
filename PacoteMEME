#Criando memes

install.packages("meme")

require("meme")

#Meme de "Cálculo"

calculo<-"http://diariogaucho.rbsdirect.com.br/imagesrc/23020882.jpg?w=620"

meme(calculo, "Quando eu lembro que terei", "que fazer Cálculo IIB")

#Meme "Volta às aulas"

aula <- "https://www.ocanal.com.br/wp-content/uploads/2019/01/wp-image-2068778646.jpg"

meme(aula,"As aulas já", "vão voltar!")

#Meme "Quero falar com você"

falar<- "https://s2.glbimg.com/5MMAaRr7DCc18-7x8W4BH0IU37w=/640x424/top/i.glbimg.com/og/ig/infoglobo/f/original/2019/01/16/hana2.png"

meme(falar, "Quero falar com você", "depois da aula,querida")
